    "nodemon": "^2.0.7",
    "postgrator-cli": "^3.3.0",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.23.3",
    "jsonwebtoken": "^9.0.0",
    "knex": "^0.21.19",
    "morgan": "^1.10.0",
